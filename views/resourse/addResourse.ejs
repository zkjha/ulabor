<div class="margin30">
    <form class="add">
        <div class="addInput">
            <label  class="addLabel mustInput"><i style="color:red">*</i><span class="i18n" data-title="resourseName">资源名称</span>:</label>
            <input id="strName" type="text" class="form-control i18n-input" selectattr="resourseName_placeholder"  placeholder="请输入资源名称">
        </div>
        <div class="addInput ">
            <label class="addLabel mustInput"><i style="color:red">*</i><span class="i18n" data-title="resourseType">资源类型</span>:</label>
            <!--<input id="strType" type="text" class="form-control" placeholder="请输入资源类型">-->
            <select id="strType" class="form-control">
                <option value="1" class="i18n" data-title="objectType1" selected>耗材</option>
                <option  class="i18n" data-title="objectType2" value="2">试剂</option>
                <option class="i18n" data-title="objectType3" value="3">动物</option>
                <option class="i18n" data-title="objectType4" value="4">仪器</option>
            </select>

        </div>
        <div class="addInput ">

            <label class="addLabel mustInput"><i style="color:red">*</i><span class="i18n" data-title="unit">单位</span>:</label>
            <input id="strUnit" type="text" class="form-control i18n-input" selectattr="unit_placeholder" placeholder="请输入单位">

        </div>
        <div class="addInput ">
            <label class="addLabel mustInput"><i style="color:red">*</i><span class="i18n" data-title="strSpecifications">获取规格</span>:</label>
            <input id="strSpecifications" type="text" class="form-control i18n-input" selectattr="strSpecifications_placeholder" placeholder="请输入获取规格">

        </div>
