            <!--<input id="strDealer" type="text" class="form-control" placeholder="请输入获取经销商">-->

        <!--</div>-->
        <!--<div class="addInput">-->
            <!--<label class="addLabel">获取数据来源:</label>-->
            <!--<input id="strComeFrome" type="text" class="form-control" placeholder="请输入获取数据来源">-->

        <!--</div>-->
        <div class="addInput">
            <label class="addLabel i18n" data-title="strRemarks">获取备注:</label>
            <input id="strRemarks" type="text" class="form-control i18n-input" selectattr="strRemarks_placeholder" placeholder="请输入获取备注">

        </div>
