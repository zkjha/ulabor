            <!--<input id="decMoney" type="text" class="form-control" placeholder="请输入单价">-->

        <!--</div>-->
        <div class="addInput">
            <label class="addLabel i18n" data-title="iNumber">库存:</label>
            <input id="iNumber" type="text" class="form-control i18n-input" selectattr="iNumber_placeholder" placeholder="请输入库存">

        </div>
        <div class="addInput">
            <label class="addLabel i18n" data-title="strCompany">公司品牌:</label>
            <input id="strCompany" type="text" class="form-control i18n-input" selectattr="strCompany_placeholder" placeholder="请输入公司品牌">

        </div>
        <div class="addInput">
            <label class="addLabel i18n" data-title="strModel">公司型号:</label>
            <input id="strModel" type="text" class="form-control i18n-input" selectattr="strModel_placeholder" placeholder="请输入公司型号">

        </div>
        <!--<div class="addInput">-->
            <!--<label class="addLabel">存放位置:</label>-->
            <!--<input id="strPosition" type="text" class="form-control" placeholder="请输入存放位置">-->

        <!--</div>-->
        <!--<div class="addInput">-->
